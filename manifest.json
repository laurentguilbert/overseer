{
  "name": "Overseer",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Oversee a list of predefined pages from a single one in a mosaic-like display.",
  "author": "Laurent Guilbert",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_title": "Overseer",
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    }
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
